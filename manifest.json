{
    "manifest_version": 3,
    "name": "Tab Manager",
    "version": "1.0",
    "description": "Extracts titles and URLs of open tabs",
    "permissions": [
        "contextMenus",
        "tabs",
        "activeTab",
        "windows",
        "scripting",
        "storage",
        "cookies",
        "bookmarks"
    ],
    "action": {
        "default_popup": "vue-project/dist/popup.html",
        "default_icon": "./icon/icon.png",
        "default_title": "Tab Manager"
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "./icon/icon.png",
        "48": "./icon/icon.png",
        "128": "./icon/icon.png"
    },
    "web_accessible_resources": [{
        "resources": [
            "js/vue.js",
            "vue-project/dist/*",
            "vue-project/dist/assets/*",
            "vue-project/dist/favicon.ico"
        ],
        "matches": ["<all_urls>"]
    }],
    "commands": {
        "move_tab_to_last": {
            "suggested_key": {
                "default": "Ctrl+0"
            }, "description": "move current tab to end"
        },
        "move_tab_to_first": {
            "suggested_key": {
                "default": "Ctrl+9"
            },
            "description": "move current tab to first"
        },
        "move_recent_tab_to_current": {
            "suggested_key": {
                "default": "Ctrl+7"
            },
            "description": "move previously focused tab to right of current tab"
        },
        "duplicate_tab": {
            "suggested_key": {
                "default": "Ctrl+D"
            },
            "description": "duplicate the current tab"
        }
    }
}